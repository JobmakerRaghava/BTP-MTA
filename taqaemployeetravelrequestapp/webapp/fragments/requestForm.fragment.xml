<c:FragmentDefinition xmlns:form="sap.ui.layout.form"
    xmlns="sap.m"
    xmlns:c="sap.ui.core">
    <Dialog title="{i18n>TravelReqForm}" resizable="true">
        <content>
            <Panel>
                <FlexBox alignItems="Center">
                    <Label text="{i18n>TravelType}" showColon="true"></Label>
                    <ComboBox selectionChange="onComboBoxSelectionChange" items="{travelType>/}" selectedKey="{visible>/trvtype}">
                        <c:Item key="{travelType>key}" text="{travelType>text}"></c:Item>
                    </ComboBox>
                </FlexBox>
            </Panel>

            <Panel visible="{visible>/type}" headerText="{i18n>Travelinfo}">
                <form:SimpleForm columnsL="4" columnsM="3" editable="true" layout="ColumnLayout">
                    <Label text="{i18n>ID}" visible="false"></Label>
                    <Input value="{form>/ID}" required="true" visible="false"></Input>
                    <Label text="{i18n>reissurance}" visible="{visible>/reissurance}"></Label>
                    <ComboBox items="{Reissuance>/}" required="true" selectedKey="{form>/ReissuranceRefNo}" visible="{visible>/reissurance}">
                        <c:Item key="{Reissuance>TravelId}" text="{Reissuance>TravelId}"></c:Item>
                    </ComboBox>
                    <!-- <Input value="MDDD/UAE/04/2024_01" required="true" visible="{visible>/reissurance}"></Input> -->
                    <Label text="{i18n>FocalPerson}" visible="{visible>/focalperson}"></Label>
                    <Input value="{form>/FocalPerson}" required="true" visible="{visible>/focalperson}"></Input>
                    <Label text="{i18n>TravelDetails}"></Label>
                    <Input value="{form>/TravelDetails}" required="true"></Input>
                    <Label text="{i18n>DepartureDate}"></Label>
                    <DatePicker required="true" value="{form>/DepartureDate}" valueFormat="yyyy-MM-dd"></DatePicker>
                    <Label text="{i18n>Sector1}"></Label>
                    <Input value="{form>/Sector}" required="true"></Input>
                    <Label text="{i18n>Time1}"></Label>
                    <!-- <Input value="{}"></Input> -->
                    <TimePicker required="true" value="{form>/Time}"></TimePicker>
                    <Label text="{i18n>ReturnDate}"></Label>
                    <DatePicker required="true" value="{form>/ReturnDate}" valueFormat="yyyy-MM-dd"></DatePicker>
                    <Label text="{i18n>Sector}"></Label>
                    <Input value="{form>/Sector2}" required="true"></Input>
                    <Label text="{i18n>Time}"></Label>
                    <TimePicker required="true" value="{form>/Time2}"></TimePicker>
                    <Label text="{i18n>TravelCountry}"></Label>
                    <Input value="{form>/TravelCountry}" required="true"></Input>
                    <Label text="{i18n>TrainingStartDate}" visible="{visible>/TrainingDate}"></Label>
                    <DatePicker value="{form>/TrainingStartDate}" required="true" valueFormat="yyyy-MM-dd" visible="{visible>/TrainingDate}"></DatePicker>
                    <Label text="{i18n>TrainingEndDate}" visible="{visible>/TrainingDate}"></Label>
                    <DatePicker value="{form>/TrainingEndDate}" required="true" valueFormat="yyyy-MM-dd" visible="{visible>/TrainingDate}"></DatePicker>
                    <Label text="{i18n>AirportCity}"></Label>
                    <Input value="{form>/AirportCity}" required="true"></Input>
                    <Label text="{i18n>ReasonforTravel}"></Label>
                    <Input value="{form>/ReasonForTravel}" required="true"></Input>
                    <Label text="{i18n>LocalMobileNumber}"></Label>
                    <Input value="{form>/LocalModbileNumber}" type="Number"></Input>
                    <Label text="{i18n>OverseasMobNo}"></Label>
                    <Input value="{form>/OverseasMobileNumber}" type="Number"></Input>
                    <Label text="{i18n>SeatPreference}"></Label>
                    <Input value="{form>/SeatPreference}"></Input>
                    <Label text="{i18n>Mealpreference}"></Label>
                    <Input value="{form>/MealPreference}"></Input>
                    <Label text="{i18n>Visarequirement}"></Label>
                    <Input value="{form>/VisaRequirement}"></Input>
                    <Label text="{i18n>FrequentflyerNo}"></Label>
                    <Input value="{form>/FrequentFlyerNumber}"></Input>
                    <Label text="{i18n>Transportation}"></Label>
                    <Input value="{form>/Transportation}"></Input>


                </form:SimpleForm>
            </Panel>
            <Panel headerText="{i18n>travellerinfo}" visible="{visible>/type}">
                <form:SimpleForm columnsL="4" columnsM="3" editable="true" layout="ColumnLayout">
                    <Label text="{i18n>ClassEconomybusiness}"></Label>
                    <Input value="{form>/Class}"></Input>
                    <Label text="{i18n>ReferenceNo}" visible="{visible>/ReferenceNo}"></Label>
                    <Input value="{form>/ReferenceNo}" type="Number" required="true" visible="{visible>/ReferenceNo}"></Input>
                    <Label text="{i18n>CodeDivTerritory}"></Label>
                    <Input value="{form>/CodeDiv}"></Input>
                    <Label text="{i18n>ExpenseCode}"></Label>
                    <Input value="{form>/ExpenseCode}" required="true"></Input>
                    <Label text="{i18n>ProjectCode}"></Label>
                    <Input value="{form>/ProjectCode}" required="true"></Input>
                    <Label text="{i18n>Designation}"></Label>
                    <Input value="{form>/Designation}" required="true"></Input>
                    <Label text="{i18n>Age}"></Label>
                    <Input value="{form>/Age}" type="Number"></Input>
                    <Label text="{i18n>Region}" visible="{visible>/Region}"></Label>
                    <Input value="{form>/Region}" visible="{visible>/Region}"></Input>
                    <Label text="{i18n>BusinessStartDate}" visible="{visible>/BusinessStartDate}"></Label>
                    <DatePicker value="{form>/BusStartDate}" required="true" valueFormat="yyyy-MM-dd" visible="{visible>/BusinessStartDate}"></DatePicker>
                    <Label text="{i18n>BusinessEndDate}" visible="{visible>/BusinessEndDate}"></Label>
                    <DatePicker value="{form>/BusEndDate}" required="true" valueFormat="yyyy-MM-dd" visible="{visible>/BusinessEndDate}"></DatePicker>
                    <Label text="{i18n>Hotel}" visible="{visible>/Hotel}"></Label>
                    <Input value="{form>/Hotel}" visible="{visible>/Hotel}"></Input>
                    <Label text="{i18n>ExitReEntry}" visible="{visible>/ExitReEntry}"></Label>
                    <DatePicker value="{form>/ExitReEntry}" visible="{visible>/ExitReEntry}" valueFormat="yyyy-MM-dd"></DatePicker>
                    <Label text="{i18n>TicketCount}" visible="{visible>/ticketcount}"></Label>
                    <Input value="{form>/TicketCount}" required="true" visible="{visible>/ticketcount}" type="Number"></Input>
                    <Label text="{i18n>PreviousfareAmount}" visible="{visible>/Preamount}"></Label>
                    <Input value="{form>/PrevFareAmt}" type="Number" required="true" visible="{visible>/Preamount}"></Input>
                </form:SimpleForm>
            </Panel>
            <Panel visible="{visible>/familytable}">
                <!-- <ScrollContainer width="100%" vertical="true" focusable="true"> -->
                <content>
                    <Table growing="true" id="idTablefamily" sticky="ColumnHeaders" headerText="{i18n>FamilyDetails}">
                        <columns>
                            <Column styleClass="border">
                                <Label text="{i18n>Name}"></Label>
                            </Column>
                            <Column styleClass="border">
                                <Label text="{i18n>Relationship}"></Label>
                            </Column>
                            <Column styleClass="border">
                                <Label text="{i18n>Phone}"></Label>
                            </Column>
                            <Column styleClass="border">
                                <Label text="{i18n>Gender}"></Label>
                            </Column>
                            <Column styleClass="border">
                                <Label text="{i18n>DateofBirth}"></Label>
                            </Column>

                        </columns>
                    </Table>
                </content>
                <!-- </ScrollContainer> -->
            </Panel>
        </content>
        <buttons>
            <Button text="{i18n>save}" press="onSaveAsDraftButtonPress" type="Emphasized" visible="{visible>/draft}"></Button>
            <Button text="{i18n>submit}" press="onSaveAsDraftButtonPress" type="Emphasized" visible="{visible>/submit}"></Button>
            <Button text="{i18n>delete}" press="onDeleteButtonPress" type="Emphasized" visible="{visible>/delete}"></Button>
            <Button text="{i18n>close}" type="Emphasized" press="onCloseButtonPress"></Button>
        </buttons>
    </Dialog>
</c:FragmentDefinition>