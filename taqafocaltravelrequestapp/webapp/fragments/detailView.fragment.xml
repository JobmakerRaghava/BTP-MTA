<c:FragmentDefinition xmlns:f="sap.f"
    xmlns="sap.m"
    xmlns:c="sap.ui.core"
    xmlns:form="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns:l="sap.ui.layout">
    <Dialog contentHeight="70%" contentWidth="80%">
        <IconTabBar id="iconTabBar" select="onTabSelect" backgroundDesign="Transparent" headerMode="Standard">
            <items>
                <IconTabFilter icon="sap-icon://detail-more" iconColor="Positive" design="Horizontal" key="tab1" text="{i18n>RequestDetails}">
                    <Panel >
                        <form:SimpleForm columnsL="4" columnsM="3" editable="true" layout="ColumnLayout">
                            <Label text="Select Travel Agents" labelFor="travelagency" />
                            <MultiInput id="travelagency" valueHelpRequest="handleValueHelp" showValueHelp="true">
                                <customData>
                                    <core:CustomData key="travelAgency" value="true" />
                                </customData>
                            </MultiInput>
                            <Label text="Select Purchase Groups" labelFor="purchasegroup" />
                            <MultiInput id="purchasegroup" valueHelpRequest="handleValueHelp" showValueHelp="true">
                                <customData>
                                    <core:CustomData key="purchasegroup" value="true" />
                                </customData>
                            </MultiInput>
                            <Label text="Select Purchase Organisations" labelFor="purchaseorg" />
                            <MultiInput id="purchaseorg" valueHelpRequest="handleValueHelp" showValueHelp="true">
                                <customData>
                                    <core:CustomData key="purchaseorg" value="true" />
                                </customData>
                            </MultiInput>
                        </form:SimpleForm>
                        <Button text="Intiate Request" press="onInitiateProcess" class="sapUiSmallMarginBeginEnd sapUiMediumMarginBottom" />
                    </Panel>
                    <Panel headerText="{i18n>EMP}" visible="{visible>/emp}">
                        <form:SimpleForm columnsL="4" columnsM="3" editable="true" layout="ColumnLayout">
                            <Label text="{i18n>ID}" visible="false"></Label>
                            <Input value="{form>/ID}" visible="false"></Input>
                            <Label text="{i18n>EmployeeNo}" showColon="true"></Label>
                            <Input value="{form>/EmployeeID}" editable="false"></Input>
                            <Label text="{i18n>FirstName}" showColon="true"></Label>
                            <Input value="{form>/EmployeeFirstName}" editable="false"></Input>
                            <Label text="{i18n>MiddleName}"></Label>
                            <Input value="{form>/EmployeeMiddleName}" editable="false"></Input>
                            <Label text="{i18n>LastName}"></Label>
                            <Input value="{form>/EmployeeLastName}" editable="false"></Input>
                            <Label text="{i18n>EmployeeFullName}"></Label>
                            <Input value="{form>/EmployeeFirstName} {form>/EmployeeMiddleName} {form>/EmployeeLastName} " editable="false"></Input>
                            <Label text="{i18n>Gender}"></Label>
                            <Input value="{form>/Gender}" editable="false"></Input>
                            <Label text="{i18n>JobDetails}"></Label>
                            <Input value="" editable="false"></Input>
                            <Label text="{i18n>LeaveSchedule}"></Label>
                            <Input value="{form>/LeaveSchedule}" editable="false"></Input>
                            <Label text="{i18n>CompanyCode}"></Label>
                            <Input value="{form>/CompanyCode}" editable="false"></Input>
                            <Label text="{i18n>HomeCountry}"></Label>
                            <Input value="{form>/HomeCountry}" editable="false"></Input>
                            <Label text="{i18n>Department}"></Label>
                            <Input value="{form>/Deparment}" editable="false"></Input>
                            <Label text="{i18n>Division}"></Label>
                            <Input value="{form>/Division}" editable="false"></Input>
                            <Label text="{i18n>Function}"></Label>
                            <Input value="{form>/Function}" editable="false"></Input>
                            <Label text="{i18n>Location}"></Label>
                            <Input value="{form>/Location}" editable="false"></Input>
                            <Label text="{i18n>DateofBirth}"></Label>
                            <DatePicker value="{ path :'form>/DateOfBirth'
                              }" editable="false"></DatePicker>
                            <!-- 
                                <Label text="{i18n>ContactNo}"></Label>
                                <Input value="{form>/ContactNo}"></Input> -->
                            <!--  -->
                            <Label text="{i18n>ReportingManager}"></Label>
                            <Input value="{form>/ReportingManager}" editable="false"></Input>
                            <Label text="{i18n>Grade}"></Label>
                            <Input value="" editable="false"></Input>
                        </form:SimpleForm>
                    </Panel>
                    <Panel visible="{visible>/type}" headerText="{i18n>Travelinfo}">
                        <form:SimpleForm columnsL="4" columnsM="3" editable="true" layout="ColumnLayout">
                            <!-- <Label text="{i18n>ID}" visible="false"></Label>
                            <Input value="{form>/ID}" editable="false" visible="false"></Input> -->
                            <Label text="{i18n>TripNumber}"></Label>
                            <Input value="{form>/TravelId}" editable="false"></Input>
                            <Label text="{i18n>TravelType}"></Label>
                            <Input value="{form>/TravelType}" editable="false"></Input>
                            <Label text="{i18n>reissurance}" visible="{visible>/reissurance}"></Label>
                            <Input value="{form>/ReissuranceRefNo}" editable="false" visible="{visible>/reissurance}"></Input>
                            <Label text="{i18n>FocalPerson}" visible="{visible>/focalperson}"></Label>
                            <Input value="{form>/FocalPerson}" editable="false" visible="{visible>/focalperson}"></Input>
                            <Label text="{i18n>TravelDetails}"></Label>
                            <Input value="{form>/TravelDetails}" editable="false"></Input>
                            <Label text="{i18n>DepartureDate}"></Label>
                            <DatePicker editable="false" value="{form>/DepartureDate}" valueFormat="yyyy-MM-dd"></DatePicker>
                            <Label text="{i18n>Sector1}"></Label>
                            <Input value="{form>/Sector}" editable="false"></Input>
                            <Label text="{i18n>Time1}"></Label>
                            <!-- <Input value="{}"></Input> -->
                            <TimePicker editable="false" value="{form>/Time}"></TimePicker>
                            <Label text="{i18n>ReturnDate}"></Label>
                            <DatePicker editable="false" value="{form>/ReturnDate}" valueFormat="yyyy-MM-dd"></DatePicker>
                            <Label text="{i18n>Sector}"></Label>
                            <Input value="{form>/Sector2}" editable="false"></Input>
                            <Label text="{i18n>Time}"></Label>
                            <TimePicker editable="false" value="{form>/Time2}"></TimePicker>
                            <Label text="{i18n>TravelCountry}"></Label>
                            <Input value="{form>/TravelCountry}" editable="false"></Input>
                            <Label text="{i18n>TrainingStartDate}" visible="{visible>/TrainingDate}"></Label>
                            <DatePicker value="{form>/TrainingStartDate}" editable="false" valueFormat="yyyy-MM-dd" visible="{visible>/TrainingDate}"></DatePicker>
                            <Label text="{i18n>TrainingEndDate}" visible="{visible>/TrainingDate}"></Label>
                            <DatePicker value="{form>/TrainingEndDate}" editable="false" valueFormat="yyyy-MM-dd" visible="{visible>/TrainingDate}"></DatePicker>
                            <Label text="{i18n>AirportCity}"></Label>
                            <Input value="{form>/AirportCity}" editable="false"></Input>
                            <Label text="{i18n>ReasonforTravel}"></Label>
                            <Input value="{form>/ReasonForTravel}" editable="false"></Input>
                            <Label text="{i18n>LocalMobileNumber}"></Label>
                            <Input value="{form>/LocalModbileNumber}" type="Number" editable="false"></Input>
                            <Label text="{i18n>OverseasMobNo}"></Label>
                            <Input value="{form>/OverseasMobileNumber}" editable="false" type="Number"></Input>
                            <Label text="{i18n>SeatPreference}"></Label>
                            <Input value="{form>/SeatPreference}" editable="false"></Input>
                            <Label text="{i18n>Mealpreference}"></Label>
                            <Input value="{form>/MealPreference}" editable="false"></Input>
                            <Label text="{i18n>Visarequirement}"></Label>
                            <Input value="{form>/VisaRequirement}" editable="false"></Input>
                            <Label text="{i18n>FrequentflyerNo}"></Label>
                            <Input value="{form>/FrequentFlyerNumber}" editable="false"></Input>
                            <Label text="{i18n>Transportation}"></Label>
                            <Input value="{form>/Transportation}" editable="false"></Input>
                        </form:SimpleForm>
                    </Panel>
                    <Panel headerText="{i18n>travellerinfo}" visible="{visible>/type}">
                        <form:SimpleForm columnsL="4" columnsM="3" editable="true" layout="ColumnLayout">
                            <Label text="{i18n>ClassEconomybusiness}"></Label>
                            <Input value="{form>/Class}" editable="false"></Input>
                            <Label text="{i18n>ReferenceNo}" visible="{visible>/ReferenceNo}"></Label>
                            <Input value="{form>/ReferenceNo}" type="Number" editable="false" visible="{visible>/ReferenceNo}"></Input>
                            <Label text="{i18n>CodeDivTerritory}"></Label>
                            <Input value="{form>/CodeDiv}" editable="false"></Input>
                            <Label text="{i18n>ExpenseCode}"></Label>
                            <Input value="{form>/ExpenseCode}" editable="false"></Input>
                            <Label text="{i18n>ProjectCode}"></Label>
                            <Input value="{form>/ProjectCode}" editable="false"></Input>
                            <Label text="{i18n>Designation}"></Label>
                            <Input value="{form>/Designation}" editable="false"></Input>
                            <Label text="{i18n>Age}"></Label>
                            <Input value="{form>/Age}" editable="false" type="Number"></Input>
                            <Label text="{i18n>Region}" visible="{visible>/Region}"></Label>
                            <Input value="{form>/Region}" editable="false" visible="{visible>/Region}"></Input>
                            <Label text="{i18n>BusinessStartDate}" visible="{visible>/BusinessStartDate}"></Label>
                            <DatePicker value="{form>/BusStartDate}" editable="false" valueFormat="yyyy-MM-dd" visible="{visible>/BusinessStartDate}"></DatePicker>
                            <Label text="{i18n>BusinessEndDate}" visible="{visible>/BusinessEndDate}"></Label>
                            <DatePicker value="{form>/BusEndDate}" editable="false" valueFormat="yyyy-MM-dd" visible="{visible>/BusinessEndDate}"></DatePicker>
                            <Label text="{i18n>Hotel}" visible="{visible>/Hotel}"></Label>
                            <Input value="{form>/Hotel}" editable="false" visible="{visible>/Hotel}"></Input>
                            <Label text="{i18n>ExitReEntry}" visible="{visible>/ExitReEntry}"></Label>
                            <DatePicker value="{form>/ExitReEntry}" editable="false" visible="{visible>/ExitReEntry}" valueFormat="yyyy-MM-dd"></DatePicker>
                            <Label text="{i18n>TicketCount}" visible="{visible>/ticketcount}"></Label>
                            <Input value="{form>/TicketCount}" editable="false" visible="{visible>/ticketcount}" type="Number"></Input>
                            <Label text="{i18n>PreviousfareAmount}" visible="{visible>/Preamount}"></Label>
                            <Input value="{form>/PrevFareAmt}" type="Number" editable="false" visible="{visible>/Preamount}"></Input>
                        </form:SimpleForm>
                    </Panel>
                     <Panel visible="{visible>/familytable}">
                <!-- <ScrollContainer width="100%" vertical="true" focusable="true"> -->
                <content>
                    <Table growing="true" id="idTablefamily" sticky="ColumnHeaders" headerText="{i18n>FamilyDetails}" >
                        <columns>
                            <Column styleClass="border">
                                <Label text="{i18n>name}"></Label>
                            </Column>
                            <Column styleClass="border">
                                <Label text="{i18n>Relationship}"></Label>
                            </Column>
                            <Column styleClass="border">
                                <Label text="{i18n>Phone}"></Label>
                            </Column>
                            <Column styleClass="border">
                                <Label text="{i18n>Gender}"></Label>
                            </Column>
                            <Column styleClass="border">
                                <Label text="{i18n>DateofBirth}"></Label>
                            </Column>
                           
                        </columns>
                    </Table>
                </content>
                <!-- </ScrollContainer> -->
            </Panel>
                </IconTabFilter>
                <!-- <IconTabSeparator icon="sap-icon://open-command-field" /> -->
                <IconTabFilter key="tab2" text="{i18n>TravelAgencyResponse}" enabled="{visible>/tab2}" design="Horizontal" icon="sap-icon://travel-itinerary" iconColor="Critical">
                    <form:SimpleForm columnsL="4" columnsM="3" editable="true" layout="ColumnLayout">
                        <Label text="{i18n>Airline}" />
                        <Input value="{selectedRowModel>/airline}" editable="false" />
                        <Label text="{i18n>Sector}" />
                        <Input value="{selectedRowModel>/sector}" editable="false" />
                        <Label text="{i18n>TravelDate}" />
                        <Input value="{selectedRowModel>/travelDate}" editable="false" />
                        <Label text="{i18n>TicketNo}" />
                        <Input value="{selectedRowModel>/ticketNo}" editable="false" />
                        <Label text="{i18n>BaseFare}" />
                        <Input value="{selectedRowModel>/baseFare}" editable="false" />
                        <Label text="{i18n>Taxes}" />
                        <Input value="{selectedRowModel>/taxes}" editable="false" />
                        <Label text="{i18n>Currency}" />
                        <Input value="{selectedRowModel>/currency}" editable="false" />
                        <Label text="{i18n>Comments}" />
                        <Input value="{selectedRowModel>/comments}" editable="false" />
                        <Label text="{i18n>Download}" />
                        <Button text="Download" />
                    </form:SimpleForm>
                </IconTabFilter>
            </items>
        </IconTabBar>
        <buttons>
            <Button text="{i18n>close}" press="onCloseButtonPress"></Button>
        </buttons>
    </Dialog>
</c:FragmentDefinition>